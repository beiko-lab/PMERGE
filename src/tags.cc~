#include "tags.h"

Tag::Tag()  { 
    this->id         = 0;
    this->seq        = "";
}

Tag::~Tag() { 

}
int Tag::add_dist(const int id, const int dist) {
    //
    // Store the ID and distance as a pair, ID in the first position,
    // dist in the second.
    //
    pair<int, int> p(id, dist);
    this->dist.push_back(p);

    return 0;
}

